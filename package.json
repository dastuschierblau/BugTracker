    "express-validator": "^6.4.0",
    "helmet": "^3.21.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
